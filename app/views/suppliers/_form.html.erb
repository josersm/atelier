<%= simple_form_for(supplier) do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :description %>
  <%= f.input :email %>
  <%= f.input :phone_number %>
  <%= f.input :price_rating %>
  <%= f.input :sustainability_rating %>
  <%= f.input :minimum_quantity %>
  <%= f.input :photo, as: :file %>
  <%= f.submit label, class: "btn btn-primary" %>
<% end %>
